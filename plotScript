set term postscript enhanced color

set output "testing_shit.ps"


set title "Density Map of Traffic"
unset key
set tic scale 0

set palette rgbformula  23,28,3
set cbrange [0:1]
set cblabel "Density"
set xlabel "Grid Point on Road"
set ylabel "Time Interval"
set view map
splot "heatmap.dat" matrix with image

pause -1 "Hit any key to continue"